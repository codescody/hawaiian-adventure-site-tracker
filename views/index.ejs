<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>Hawaiian Islands Adventure Site Tracker</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="/css/style.css">
    </head>
    <body>
        <nav>
            <img id="home-icon" src="https://static.vecteezy.com/system/resources/previews/000/585/465/non_2x/mountain-and-water-logo-business-template-vector.jpg">
            <h1 id="home-title">Hawaiian Adventure Tracker</h1>
            <a id="add-link" href="/sites/new"><img id="add-button" src="https://cdn0.iconfinder.com/data/icons/round-ui-icons/512/add_green.png"></a>
        </nav>
        <div id="items-container">
        <% sites.forEach(site => { %>
            <div id="home-container">
            <%= site.name %>,
            <%= site.island %>
            <a href="/sites/<%=site._id%>"><img src=<%= site.image %>></a>
            </div>
        <% }) %>
        </div>
        <script src="" async defer></script>
    </body>
</html>